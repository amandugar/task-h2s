<html>
  <head>
    <title>Table</title>
  </head>
  <body>
    <div><%- include('table.ejs') %></div>
  </body>
  <style>
    table {
      border-collapse: collapse;
      width: 100%;
    }

    th,
    td {
      text-align: center;
      padding: 8px;
    }

    tr:nth-child(even) {
      background-color: #f2f2f2;
    }

    th {
      background-color: #4caf50;
      color: white;
    }
  </style>
</html>
